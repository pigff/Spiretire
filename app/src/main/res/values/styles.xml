<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/grey_238_color_code</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="TitleTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black_62_color_code</item>
        <item name="android:maxLines">2</item>
        <item name="android:ems">20</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="contentTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lines">2</item>
        <item name="android:ellipsize">end</item>
        <item name="android:ems">40</item>
        <item name="android:textSize">10sp</item>
    </style>


    <style name="RegisterEditStyle">
        <item name="android:background">@drawable/border_bottom</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:layout_marginRight">@dimen/dp_10</item>
        <item name="android:textColor">@color/grey_238_color_code</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="CardTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black_116_color_code</item>
        <item name="android:maxLines">2</item>
        <item name="android:ems">18</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="CardLinerStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
    </style>

    <style name="DividingDashLineStyle">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@drawable/bg_dash_line</item>
    </style>

    <style name="DividingLineStyle">
        <item name="android:layout_height">1px</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/grey_207_color_code</item>
    </style>

    <style name="Dividing10dpLineStyle">
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/grey_238_color_code</item>
    </style>

    <style name="AgenceCardTvStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/dp_15</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black_80_color_code</item>
        <item name="android:lines">1</item>
    </style>

    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>

    <style name="DrawerAnimationPreview">
        <item name="android:windowEnterAnimation">@anim/translate_x_in</item>
        <item name="android:windowExitAnimation">@anim/translate_x_out</item>
    </style>

    <style name="editpopwindow_anim_style">
        <item name="android:windowEnterAnimation">@anim/popshow_anim</item>
        <!-- 指定显示的动画xml -->
        <item name="android:windowExitAnimation">@anim/pophidden_anim</item>
        <!-- 指定消失的动画xml -->
    </style>

    <style name="Dialog" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="StyleFilePushCategoryText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:textColor">@color/blue_59_color_code</item>
    </style>

    <style name="FilePushNumberLabel">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingLeft">@dimen/dp_2</item>
        <item name="android:paddingRight">@dimen/dp_2</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/shape_solid_circle_orange248</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <dimen name="dp_10">10dp</dimen>
</resources>
